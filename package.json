{
  "name": "webapp2pdf",
  "version": "0.1.0",
  "description": "CLI that prints a website with JavaScript generated content into a PDF.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "./node_modules/.bin/tsc -p .",
    "build:watch": "./node_modules/.bin/tsc -p . -w",
    "postinstall": "npm run build"
  },
  "preferGlobal": true,
  "bin": {
    "webapp2pdf": "./dist/webapp2pdf.js"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tobiasholler/webapp2pdf.git"
  },
  "author": "Tobias Holler",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tobiasholler/webapp2pdf/issues"
  },
  "homepage": "https://github.com/tobiasholler/webapp2pdf#readme",
  "dependencies": {
    "puppeteer": "^2.1.1",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "@types/puppeteer": "^2.0.0",
    "@types/node": "^13.7.0",
    "@types/yargs": "^15.0.3",
    "typescript": "^3.7.5"
  }
}
